{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}, {
			"path": "pages/category/category",
			"style": {
				"navigationBarTitleText": "分类"
			}
		}, {
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "京喜"
			}
		}, {
			"path": "pages/cart/cart",
			"style": {
				"navigationBarTitleText": "购物车"
			}
		}, {
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "未登录"
			}
		}
	],
	"tabBar": {
		"selectedColor": "#f10001",
		"list": [{
			"pagePath": "pages/index/index",
			"text": "首页",
			"selectedIconPath": "static/images/tab-index-selected.png",
			"iconPath": "static/images/tab-index.png"
		}, {
			"pagePath": "pages/category/category",
			"text": "分类",
			"selectedIconPath": "static/images/tab-category-selected.png",
			"iconPath": "static/images/tab-category.png"
		}, {
			"pagePath": "pages/home/home",
			"text": "京喜",
			"iconPath": "static/images/tab-home.png"
		}, {
			"pagePath": "pages/cart/cart",
			"text": "购物车",
			"selectedIconPath": "static/images/tab-cart-selected.png",
			"iconPath": "static/images/tab-cart.png"
		}, {
			"pagePath": "pages/login/login",
			"text": "未登录",
			"iconPath": "static/images/tab-login.png"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#d80003",
		"backgroundColor": "#F8F8F8"
	}
}
